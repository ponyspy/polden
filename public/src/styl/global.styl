body
	margin 0
	padding 0

.main_block
	width 1140px
	// outline 1px solid red

.menu_block
	// outline 1px solid red
	margin-top 10px
	margin-bottom 30px
	width 900px
	overflow hidden
	border-bottom 2px solid black
	a
		text-decoration none
		margin-right 80px
		display block
		float left
		font 12pt/17pt 'DinProBold'
		color rgb(0, 220, 50)
		&:last-child
			margin-right 0px
	:hover
		color black

.header_block
	margin-left 160px
	width 1280px

.head_title_block
	overflow hidden

.logo_one_img
	margin-top 15px
	width 245px
	float left

.logo_exhibition
	float left
	margin-left 30px
	width 510px
	color #ff144b
	position relative
	z-index 1

.logo_exhibition_date
	font 64pt/66pt 'DinProMedium'

.logo_exhibition_year
	font 30pt/26pt 'DinProMedium'

.logo_exhibition_title
	font 30pt/32pt 'DinProMedium'
	margin-top 25px;

.logo_two
	top 15px
	left 850px
	float left
	position absolute
	.logo_two_img
		width 200px
		position absolute
		z-index 0
	.logo_two_link
		width 200px
		height 200px
		position absolute
		z-index 999

.title_text
	width 230px
	margin-left 120px
	margin-top 15px
	float left
	font 13pt/14pt 'DinProMedium'
	hyphens: auto
	position relative
	z-index 999999

.text_green
	color rgb(0, 220, 50)

.content_block
	// outline 1px solid red
	width 900px
	margin-left 160px
	padding-bottom 50px
	overflow hidden